#include <iostream>
#include <vector>
#include <queue>
using namespace std;

vector<int> node[100];

int main() {
	cout << "Sorting Algorithm" << endl; // prints Sorting Algorithm
	int N = 6;

	node[1].push_back(5);
	node[3].push_back(4);
	node[5].push_back(4);
	node[4].push_back(2);
	node[4].push_back(6);
	node[5].push_back(2);

	for(int i=0 ; i<N ; i++){
		queue<int> q;
		int start = i;
		int visited[100] = {0,};
		q.push(start);

	}

	return 0;
}
